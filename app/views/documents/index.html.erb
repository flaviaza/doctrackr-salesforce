<div class="dashboard">
  <%= link_to 'Secure a new document', new_document_path, class: 'btn' %>
  <select>
    <option> Generate Report </option>
  </select>
  <%= link_to 'Jan 21, 2014 - Feb 21, 2014', '' %>
  <h1>Dashboard</h1>
  <img src="images/dashboard_map.png" alt="map and alerts" />
  <img src="images/dashboard_doc.png" alt="documents" />
  <img src="images/dashboard_group.png" alt="groups" />
</div>

<% if false %>
<h1>Listing documents</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Dt reference</th>
    <th>Sf reference</th>
    <th>Last event at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @documents.each do |document| %>
  <tr>
    <td><%= document.name %> </td>
    <td><%= document.dt_reference %> </td>
    <td><%= document.sf_reference %> </td>
    <td><%= document.last_event_at %> </td>
    <td><%= link_to 'Show', document %> </td>
    <td><%= link_to 'Edit', edit_document_path(document) %> </td>
    <td><%= link_to 'Destroy', document, method: :delete, data: { confirm: 'Are you sure?' } %> </td>
  </tr>
<% end %>
</table>

<br />
<% end %>


